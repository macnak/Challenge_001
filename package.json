        "dev:fixed-access:none": "ACCESS_METHOD=none tsx watch src/server/index.ts",
        "dev:fixed-access:user-pass": "ACCESS_METHOD=user-pass tsx watch src/server/index.ts",
        "dev:fixed-order": "FIXED_ORDER=1 tsx watch src/server/index.ts",
        "dev:per-page-results": "SHOW_PER_PAGE_RESULTS=1 tsx watch src/server/index.ts",
        "dev:per-page-results:strict": "SHOW_PER_PAGE_RESULTS=1 BLOCK_CONTINUE_ON_FAILURE=1 tsx watch src/server/index.ts",
        "dev:guided": "ACCESS_METHOD=basic FIXED_ORDER=1 SHOW_PER_PAGE_RESULTS=1 tsx watch src/server/index.ts",
