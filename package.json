        "fastify": "^5.7.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/node": "^22.13.1",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.19.0",
        "prettier": "^3.4.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "vite": "^6.1.0",
        "vitest": "^4.0.18"
    }
