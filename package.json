        "start:fixed-access:jwt": "ACCESS_METHOD=jwt node dist/server/index.js",
        "start:fixed-access:none": "ACCESS_METHOD=none node dist/server/index.js",
        "start:fixed-access:user-pass": "ACCESS_METHOD=user-pass node dist/server/index.js",
        "start:fixed-order": "FIXED_ORDER=1 node dist/server/index.js",
        "start:per-page-results": "SHOW_PER_PAGE_RESULTS=1 node dist/server/index.js",
        "start:per-page-results:strict": "SHOW_PER_PAGE_RESULTS=1 BLOCK_CONTINUE_ON_FAILURE=1 node dist/server/index.js",
        "start:guided": "ACCESS_METHOD=basic FIXED_ORDER=1 SHOW_PER_PAGE_RESULTS=1 node dist/server/index.js",
        "start:guided:strict": "ACCESS_METHOD=basic FIXED_ORDER=1 SHOW_PER_PAGE_RESULTS=1 BLOCK_CONTINUE_ON_FAILURE=1 node dist/server/index.js",
        "test": "vitest run",
        "lint": "eslint .",
        "format": "prettier -w ."
